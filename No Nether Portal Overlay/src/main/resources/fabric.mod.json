{
  "schemaVersion": 1,
  "id": "no_nether_portal_overlay",
  "version": "${version}",
  "name": "No Nether Portal Overlay",
  "description": "This mod removes the annoying nether portal overlay!",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/no-nether-portal-overlay/issues/",
    "sources": "https://github.com/SmushyTaco/No-Nether-Portal-Overlay/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/no_nether_portal_overlay/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.no_nether_portal_overlay.NoNetherPortalOverlay"
      }
    ],
    "modmenu": [
      "com.smushytaco.no_nether_portal_overlay.configuration_support.ModMenuIntegration"
    ]
  },
  "mixins": [
    "no_nether_portal_overlay.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.7.1+kotlin.1.6.10",
    "minecraft": "1.18.x",
    "cloth-config2": ">=6.1.48",
    "java": ">=17"
  },
  "suggests": {
    "modmenu": ">=3.0.1"
  }
}